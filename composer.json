{
  "name": "skagl/ynab-sdk-php",
  "version": "1.2.0",
  "description": "(non-official) YNAB API PHP Library",
  "keywords": [
    "swagger",
    "php",
    "sdk",
    "api",
    "ynab"
  ],
  "homepage": "https://jorijn.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jorijn Schrijvershof <jorijn@jorijn.com>",
      "homepage": "https://jorijn.com"
    },
    {
      "name": "Daniel Pieper <github@daniel-pieper.com>",
      "homepage": "https://jorijn.com"
    },
    {
      "name": "Swagger and contributors",
      "homepage": "https://github.com/swagger-api/swagger-codegen"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "YNAB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YNAB\\": "test/"
    }
  }
}
